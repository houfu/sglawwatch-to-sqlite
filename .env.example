# API Tokens for content extraction and summarization
JINA_API_TOKEN=your_jina_api_token_here
OPENAI_API_KEY=your_openai_api_key_here

# AWS S3 Configuration (for standard AWS S3)
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_DEFAULT_REGION=ap-southeast-1
S3_BUCKET_NAME=your_bucket_name_here

# Contabo Object Storage Configuration
# If you're using Contabo instead of AWS, set these variables
# S3_ENDPOINT_URL is required for Contabo
S3_ENDPOINT_URL=https://eu2.contabostorage.com
